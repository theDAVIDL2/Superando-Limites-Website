  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not IE 11",
      "not op_mini all",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@eslint/js": "9.34.0",
    "autoprefixer": "^10.4.20",
